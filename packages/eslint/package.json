{
	"name": "@gixy/eslint-plugin-unused-imports",
	"version": "0.0.0",
	"description": "privite",
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin"
	],
	"author": "@gixy/eslint",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"lint": "npm-run-all \"lint:*\"",
		"lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
		"lint:js": "eslint .",
		"test": "mocha tests --recursive",
		"update:eslint-docs": "eslint-doc-generator"
	},
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@types/eslint": "^8.56.10",
		"@types/estree": "^1.0.5",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.12.7",
		"eslint": "^9.0.0",
		"eslint-doc-generator": "^2.0.0",
		"eslint-plugin-eslint-plugin": "^6.0.0",
		"eslint-plugin-n": "^17.0.0",
		"mocha": "^11.0.0",
		"npm-run-all2": "^6.1.2",
		"tsup": "^8.5.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"requireindex": "^1.2.0"
	},
	"engines": {
		"node": "^18.18.0 || ^20.9.0 || >=21.1.0"
	},
	"peerDependencies": {
		"eslint": ">=8.57.0"
	},
	"license": "MIT"
}
