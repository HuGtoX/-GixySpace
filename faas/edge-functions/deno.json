	"compilerOptions": {
		"allowJs": true,
		"noImplicitAny": false,
		"strict": false
	},
	"imports": {
		"@std/assert": "jsr:@std/assert@^1.0.13",
		"dot-env": "https://esm.sh/jsr/@std/dotenv/load"
	}
}
